/**index.wxss**/
/* 轮播图样式 */
.swiper swiper {
	width: 750rpx;
	height: 340rpx;
  }
  
  .swiper image {
	width: 100%;
	height: 100%;
  }
  
  /* 功能图标 */
  .nav {
	display: flex;
  }
  .nav navigator {
	flex: 1;
	padding: 20rpx;
  }
  .nav navigator image {
	width: 100%;
  }
  
  /* 楼层部分 */
  .index_floor {
    overflow: hidden;
  }
  .index_floor .floor_title image {
    width: 100%;
  }
  .index_floor .floor_list navigator {
    float: left;
    width: 33.33vw;
  }
  .index_floor .floor_list navigator:nth-last-child(-n+4) {
    height: 27.45258621vw;
    border-left: 10rpx solid #fff;
  }
  .index_floor .floor_list navigator:nth-child(2),
  .index_floor .floor_list navigator:nth-child(3) {
    border-bottom: 10rpx solid #fff;
  }
  .index_floor .floor_list navigator image {
    width: 100%;
    height: 100%;
  }